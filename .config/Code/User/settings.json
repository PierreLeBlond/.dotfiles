{
  "workbench.colorTheme": "Default Dark Modern",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.limit.value": 2,
  "workbench.editor.showTabs": "single",
  "workbench.layoutControl.enabled": false,
  "window.menuBarVisibility": "compact",
  "window.customMenuBarAltFocus": false,
  "window.titleBarStyle": "custom",
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
  "window.commandCenter": false,
  "editor.lineNumbers": "relative",
  "editor.fontSize": 12,
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.autoClosingBrackets": "beforeWhitespace",
  "editor.autoClosingQuotes": "beforeWhitespace",
  "vim.foldfix": true,
  "vim.useCtrlKeys": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["u"],
      "commands": ["undo"]
    },
    {
      "before": ["<C-r>"],
      "commands": ["redo"]
    }
  ],
  "vim.useSystemClipboard": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "explorer.autoReveal": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "svelte.enable-ts-plugin": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "workbench.editor.splitSizing": "distribute",
  "workbench.editor.tabSizing": "fixed",
  "workbench.settings.useSplitJSON": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.activityBar.location": "hidden",
  "vim.startofline": false,
  "markdown-pdf.displayHeaderFooter": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.compactFolders": false,
  "jest.jestCommandLine": "pnpm run test",
  "jest.shell": "/bin/zsh",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prisma.showPrismaDataPlatformNotification": false,
  "github.copilot.editor.enableAutoCompletions": true,
  "[csharp]": {
    "editor.defaultFormatter": "csharpier.csharpier-vscode"
  },
  "codeium.enableCodeLens": false,
  "git.openRepositoryInParentFolders": "always",
  "codeium.enableConfig": {
    "*": true,
    "aspnetcorerazor": true,
    "typescript": false
  },
  "razor.format.enable": false,
  "[aspnetcorerazor]": {},
  "codeium.disableSupercomplete": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "cmake.pinnedCommands": [
    "workbench.action.tasks.configureTaskRunner",
    "workbench.action.tasks.runTask"
  ],
  "C_Cpp.formatting": "clangFormat",
  "C_Cpp.workspaceParsingPriority": "medium",
  "C_Cpp.files.exclude": {
    "ext/**/*": true,
    "**/.vscode": true,
    "**/.vs": true
  },
  "cmake.showOptionsMovedNotification": false,
  "C_Cpp.clang_format_fallbackStyle": "LLVM",
  "C_Cpp.codeAnalysis.exclude": {
    "ext/**/*": true
  },
  "glsllint.languageSettings": {
    "javascript": {
      "parser": "TSAST"
    },
    "javascriptreact": {
      "parser": "TSAST"
    },
    "typescript": {
      "parser": "TSAST"
    },
    "typescriptreact": {
      "parser": "TSAST"
    },
    "elm": {
      "parser": "REGEX",
      "patternStart": "\\[glsl",
      "patternEnd": "\\|\\]"
    }
  },
  "sherlock.userId": "1132c19c-0531-48db-8e0f-d5aebdf6a838",
  "sherlock.appRecommendations.ninja.enabled": false,
  "cSpell.language": "en,fr",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
